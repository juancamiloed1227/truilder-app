{"ast":null,"code":"import { inject, ref } from 'vue';\nimport NodeNumber from '@/components/Board/Nodes/NodeNumber.vue';\nexport default {\n  __name: 'NodeList',\n  props: {\n    nodeType: {\n      type: String,\n      required: true\n    }\n  },\n\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const props = __props;\n\n    const randomInt = (min, max) => {\n      min = Math.ceil(min);\n      max = Math.floor(max);\n      return Math.floor(Math.random() * (max - min) + min);\n    };\n\n    const editor = ref(inject('editor'));\n\n    const addNodeToBoard = values => {\n      editor.value.addNode(values.name, values.inputs, values.outputs, randomInt(50, 200), 150, values.name, {}, values.node, 'vue');\n    };\n\n    const __returned__ = {\n      props,\n      randomInt,\n      editor,\n      addNodeToBoard,\n      inject,\n      ref,\n      NodeNumber\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAkBA,SAAsBA,MAAtB,EAA8BC,GAA9B,QAAyC,KAAzC;AACA,OAAOC,UAAP,MAAuB,yCAAvB;;;;;;;;;;;;;;;;IASA,MAAMC,SAAS,GAAG,CAACC,GAAD,EAAMC,GAAN,KAAc;MAC5BD,GAAG,GAAGE,IAAI,CAACC,IAAL,CAAUH,GAAV,CAAN;MACAC,GAAG,GAAGC,IAAI,CAACE,KAAL,CAAWH,GAAX,CAAN;MACA,OAAOC,IAAI,CAACE,KAAL,CAAWF,IAAI,CAACG,MAAL,MAAiBJ,GAAG,GAAGD,GAAvB,IAA8BA,GAAzC,CAAP;IACH,CAJD;;IAMA,MAAMM,MAAM,GAAGT,GAAG,CAACD,MAAM,CAAC,QAAD,CAAP,CAAlB;;IAEA,MAAMW,cAAc,GAAIC,MAAD,IAAY;MAC/BF,MAAM,CAACG,KAAP,CAAaC,OAAb,CAAqBF,MAAM,CAACG,IAA5B,EAAkCH,MAAM,CAACI,MAAzC,EAAiDJ,MAAM,CAACK,OAAxD,EAAiEd,SAAS,CAAC,EAAD,EAAK,GAAL,CAA1E,EAAqF,GAArF,EAA0FS,MAAM,CAACG,IAAjG,EAAuG,EAAvG,EAA2GH,MAAM,CAACM,IAAlH,EAAwH,KAAxH;IACH,CAFD","names":["inject","ref","NodeNumber","randomInt","min","max","Math","ceil","floor","random","editor","addNodeToBoard","values","value","addNode","name","inputs","outputs","node"],"sources":["/Users/juancamiloespinosa/Desktop/Juan Camilo/platzi/TR/drawflow-test/s