{"ast":null,"code":"import { inject } from 'vue';\nimport FlowSelector from './FlowSelector.vue';\nexport default {\n  __name: 'AdminPanel',\n  props: {\n    databaseFlows: {\n      type: Object\n    }\n  },\n  emits: [\"close\"],\n\n  setup(__props, {\n    expose,\n    emit\n  }) {\n    expose();\n    const props = __props;\n    const editor = inject('editor');\n    const flowId = inject('flowId');\n\n    const close = () => emit('close');\n\n    const loadFlow = (id, title, content) => {\n      flowId.value = id;\n      let data = content;\n      data = data.replaceAll('closeSquare', '}');\n      data = data.replaceAll('openSquare', '{');\n      data = data.replaceAll('quote', '\"');\n      data = data.replace(/[\\r\\n]/gm, '');\n      editor.value.import(JSON.parse(data));\n      emit('close');\n    };\n\n    const __returned__ = {\n      emit,\n      editor,\n      flowId,\n      close,\n      props,\n      loadFlow,\n      inject,\n      FlowSelector\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAUA,SAAmCA,MAAnC,QAAiD,KAAjD;AACA,OAAOC,YAAP,MAAyB,oBAAzB;;;;;;;;;;;;;;;;IAGA,MAAMC,MAAM,GAAGF,MAAM,CAAC,QAAD,CAArB;IACA,MAAMG,MAAM,GAAGH,MAAM,CAAC,QAAD,CAArB;;IAEA,MAAMI,KAAK,GAAG,MAAMC,IAAI,CAAC,OAAD,CAAxB;;IAQA,MAAMC,QAAQ,GAAG,CAACC,EAAD,EAAKC,KAAL,EAAYC,OAAZ,KAAwB;MACrCN,MAAM,CAACO,KAAP,GAAeH,EAAf;MAEA,IAAII,IAAI,GAAGF,OAAX;MACAE,IAAI,GAAGA,IAAI,CAACC,UAAL,CAAgB,aAAhB,EAA+B,GAA/B,CAAP;MACAD,IAAI,GAAGA,IAAI,CAACC,UAAL,CAAgB,YAAhB,EAA8B,GAA9B,CAAP;MACAD,IAAI,GAAGA,IAAI,CAACC,UAAL,CAAgB,OAAhB,EAAyB,GAAzB,CAAP;MACAD,IAAI,GAAGA,IAAI,CAACE,OAAL,CAAa,UAAb,EAAyB,EAAzB,CAAP;MAEAX,MAAM,CAACQ,KAAP,CAAaI,MAAb,CAAoBC,IAAI,CAACC,KAAL,CAAWL,IAAX,CAApB;MACAN,IAAI,CAAC,OAAD,CAAJ;IACH,CAXD","names":["inject","FlowSelector","editor","flowId","close","emit","loadFlow","id","title","content","value","data","replaceAll","replace","import","JSON","parse"],"sources":["/Users/juancamiloespinosa/Desktop/truilder-app/src/components/Board/AdminPanel/AdminPanel.vue"],"sourcesContent":["<template>\n    <div class=\"global-container\">\n        <img src=\"@/assets/images/close-icon.svg\" @click=\"close\"/>\n        <div class=\"flows-container\" >\n            <FlowSelector v-for=\"item in props.databaseFlows.queryFlow\" :key=\"item.id\" :title=\"item.title\" @click=\"loadFlow(item.id, item.title, item.content)\"/>\n        </div>\n    </div>\n</template>\n\n<script setup>\nimport { defineEmits, defineProps, inject } from 'vue';\nimport FlowSelector from './FlowSelector.vue'\n\nconst emit = defineEmits([\"close\"])\nconst editor = inject('editor')\nconst flowId = inject('flowId')\n\nconst close = () => emit('close')\n\nconst props = defineProps({\n    databaseFlows: {\n        type: Object\n    }\n})\n\nconst loadFlow = (id, title, content) => {\n    flowId.value = id\n    \n    let data = content\n    data = data.replaceAll('closeSquare', '}')\n    data = data.replaceAll('openSquare', '{')\n    data = data.replaceAll('quote', '\"')\n    data = data.replace(/[\\r\\n]/gm, '');\n\n    editor.value.import(JSON.parse(data))\n    emit('close')\n}\n</script>\n\n<style scoped>\n.global-container {\n    width: 100%;\n    height: 100%;\n    padding: 15px;\n}\n.flows-container {\n    margin-top: 15px;\n    height: 90%;\n    display: grid;\n    grid-template-columns: 1fr 1fr 1fr 1fr;\n    column-gap: 15px;\n    row-gap: 15px;\n    overflow-y: auto;\n}\n</style>"]},"metadata":{},"sourceType":"module"}