{"ast":null,"code":"import { inject } from 'vue';\nimport FlowSelector from './FlowSelector.vue';\nexport default {\n  __name: 'AdminPanel',\n  props: {\n    databaseFlows: {\n      type: Object\n    }\n  },\n  emits: [\"close\"],\n\n  setup(__props, {\n    expose,\n    emit\n  }) {\n    expose();\n    const props = __props;\n    const editor = inject('editor');\n    let flowId = inject('flowId');\n    let flowTitle = inject('flowTitle');\n\n    const close = () => emit('close');\n\n    const loadFlow = (id, title, content) => {\n      flowId.value = id;\n      flowTitle.value = title;\n      let data = content;\n      data = data.replaceAll('closeSquare', '}');\n      data = data.replaceAll('openSquare', '{');\n      data = data.replaceAll('quote', '\"');\n      data = data.replace(/[\\r\\n]/gm, 'linebreak');\n      editor.value.import(JSON.parse(data));\n      const obj = editor.value.drawflow.drawflow.Home.data;\n      const keys = Object.keys(obj);\n\n      for (let i = 0; i < keys.length; i++) {\n        const key = keys[i];\n        Object.entries(obj[key].data).forEach(([keyI, value]) => {\n          typeof value === 'string' ? obj[key].data[keyI] = value.replaceAll('lineBreak', '\\n') : {};\n        });\n      }\n\n      emit('close');\n    };\n\n    const __returned__ = {\n      emit,\n      editor,\n      flowId,\n      flowTitle,\n      close,\n      props,\n      loadFlow,\n      inject,\n      FlowSelector\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AAUA,SAAmCA,MAAnC,QAAiD,KAAjD;AACA,OAAOC,YAAP,MAAyB,oBAAzB;;;;;;;;;;;;;;;;IAGA,MAAMC,MAAM,GAAGF,MAAM,CAAC,QAAD,CAArB;IACA,IAAIG,MAAM,GAAGH,MAAM,CAAC,QAAD,CAAnB;IACA,IAAII,SAAS,GAAGJ,MAAM,CAAC,WAAD,CAAtB;;IAEA,MAAMK,KAAK,GAAG,MAAMC,IAAI,CAAC,OAAD,CAAxB;;IAQA,MAAMC,QAAQ,GAAG,CAACC,EAAD,EAAKC,KAAL,EAAYC,OAAZ,KAAwB;MACrCP,MAAM,CAACQ,KAAP,GAAeH,EAAf;MACAJ,SAAS,CAACO,